{
  "name": "cypress-automation-framework",
  "version": "1.0.0",
  "description": "Test project with cypress automation framework",
  "main": "index.js",
  "scripts": {
    "triggerAllTests-headless": "npx cypress run",
    "triggerAllTests-headed": "npx cypress run --headed",
    "triggerAllTests-chrome": "npx cypress run --browser chrome",
    "triggerAllTests-dashboard": "npx cypress run --record --key d42e4517-1631-4a9f-9e23-272ffe4d6527",
    "triggerAllTests-webdriveruni": "npx cypress run --spec cypress/integration/webdriver-ui/*"
  },
